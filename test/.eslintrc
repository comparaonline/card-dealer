{
  "extends": "../.eslintrc",
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "HttpRecording": true,
    "fakeTime": true,
    "testObserver": true,
    "fail": true,
    "invalidateCache": true
  },
  "rules": {
    "no-param-reassign": 0,
    "no-unused-expressions": 0
  }
}
